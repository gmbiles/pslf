<div class="container">
  <div class="jumbotron app center">
    <h1>Completed Forms</h1>

    <table>
      <thead>
        <tr>
          <th>Description</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @certs.each do |cert| %>
          <tr>
            <td><%= cert.document %></td>
            <td><%= cert.description %></td>
            <td><%= link_to 'Show', cert %></td>
            <td><%= link_to 'Edit', edit_cert_path(cert) %></td>
            <td><%= link_to 'Destroy', cert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

    <%= link_to 'New Cert', new_cert_path %>
  </div>
</div>